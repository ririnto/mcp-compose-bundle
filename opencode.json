{
  "mcp": {
    "context7": {
      "type": "local",
      "command": [
        "bunx",
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "enabled": true
    },
    "fetch": {
      "type": "local",
      "command": [
        "uvx",
        "mcp-server-fetch",
        "--ignore-robots-txt"
      ],
      "enabled": true
    },
    "git": {
      "type": "local",
      "command": [
        "uvx",
        "mcp-server-git"
      ],
      "enabled": true
    },
    "markitdown": {
      "type": "local",
      "command": [
        "uvx",
        "markitdown-mcp"
      ],
      "enabled": true
    },
    "playwright": {
      "type": "local",
      "command": [
        "bunx",
        "-y",
        "@playwright/mcp@latest"
      ],
      "enabled": true
    },
    "exa": {
      "type": "remote",
      "url": "https://mcp.exa.ai/mcp",
      "enabled": true
    },
    "grep-app": {
      "type": "remote",
      "url": "https://mcp.grep.app",
      "enabled": true
    }
  }
}
