{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "fetch": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-fetch",
        "--ignore-robots-txt"
      ]
    },
    "git": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    },
    "markitdown": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "markitdown-mcp"
      ]
    },
    "playwright": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "exa": {
      "type": "http",
      "url": "https://mcp.exa.ai/mcp"
    },
    "grep-app": {
      "type": "http",
      "url": "https://mcp.grep.app"
    }
  }
}
